# Uncomment the next line to define a global platform for your project
platform :ios, '9.0'

target 'TelePatriot' do
    # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
    use_frameworks!
    
    # Pods for TelePatriot
    # see all the possibilities here: https://firebase.google.com/docs/ios/setup
    pod 'Firebase/Core', '~> 5.0'
    pod 'Firebase/Messaging'
    pod 'Firebase/Database', '~> 5.0'
    pod 'Firebase/DynamicLinks'
    pod 'Firebase/RemoteConfig'
    pod 'Firebase/Auth'
    pod 'IQKeyboardManagerSwift' #, '6.5.4'
    pod 'FirebaseUI', '~> 5.0'
    pod 'FirebaseInstanceID'
    
    pod 'FBSDKCoreKit'
    pod 'FBSDKShareKit'
    pod 'FBSDKLoginKit'

    pod 'TwilioVideo', '~> 2.4.0'
    
end

